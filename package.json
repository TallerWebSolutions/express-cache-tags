{
  "name": "express-cache-tags",
  "version": "1.0.0",
  "private": false,
  "description": "A (not yet) fully featured cache-tagging solution for node servers.",
  "license": "MIT",
  "author": "Lucas Constantino Silva <lucasconstantinosilva@gmail.com>",
  "main": "lib/index.js",
  "scripts": {
    "codecov": "codecov",
    "compile": "babel src -d lib --ignore src/**/*.test.js",
    "lint": "eslint src",
    "prepare": "yarn qa && yarn compile",
    "qa": "yarn test && yarn lint",
    "release": "standard-version",
    "test": "jest src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn qa"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleNameMapper": {
      "^express-cache-tags(.*)$": "<rootDir>/src$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "dependencies": {
    "express": "^4.16.4",
    "figures": "^2.0.0",
    "memory-cache": "^0.2.0",
    "on-finished": "^2.3.0",
    "signale": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-plugin-rewire": "^1.2.0",
    "codecov": "^3.2.0",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier-eslint-cli": "^4.7.1",
    "sort-package-json": "^1.19.0",
    "standard-version": "^8.0.1",
    "std-mocks": "^1.0.1",
    "supertest": "^3.4.2"
  }
}
